	this.dept = dept;
}
department dept1;
public emp(String name, String des, department dept1) {
	super();
	this.name = name;
	this.des = des;
	this.dept1 = dept1;
}
void m()
{
	System.out.println(name+" a "+des+" working in "+dept.dname+" department at "+" city");
